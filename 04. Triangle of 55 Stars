using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Nachaloto

{
    class Nachaloto
    {
        static void Main()
        {
            Elha(10);
        }

        static void Elha(int n)
        {
            for (int i = 0; i <= 10; i++)
            {
                Console.WriteLine(new string('*',i));
            }
        }
    }
}
